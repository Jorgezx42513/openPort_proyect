<Window x:Class="OpenPort.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenPort"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Margin="50,20,0,0">Mysql</Label>
        <Button x:Name="MySql_button" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,50,0,0" Click="MySql_button_Click">start</Button>
        <Label Margin="134,20,-84,0" Content="PHP"/>
        <Button x:Name="php_button" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="126,50,0,0" Click="MySql_button_Click" Content="start"/>
        <Label Margin="200,20,-150,0" Content="Apache"/>
        <Button x:Name="apache_button" Width="50" Height="30" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="200,50,0,0" Click="apache_button_Click" Content="start"/>
    </Grid>
</Window>
